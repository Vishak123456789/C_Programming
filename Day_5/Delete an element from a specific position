#include <stdio.h>
int main() {
    int arr[5] = {1, 2, 3, 4, 5}, pos;
    printf("Enter position (0â€“4) to delete: ");
    scanf("%d", &pos);

    for (int i = pos; i < 4; i++)
        arr[i] = arr[i + 1];

    printf("New array:\n");
    for (int i = 0; i < 4; i++)
        printf("%d ", arr[i]);
    return 0;
}
