#include <stdio.h>
int main() {
    int arr[6] = {1, 2, 3, 4, 5}, pos, val;
    printf("Enter position (0â€“5) and value to insert: ");
    scanf("%d %d", &pos, &val);

    for (int i = 5; i > pos; i--)
        arr[i] = arr[i - 1];
    arr[pos] = val;

    printf("New array:\n");
    for (int i = 0; i < 6; i++)
        printf("%d ", arr[i]);
    return 0;
}
