#include <stdio.h>

int main() {
    float avg;
    printf("Enter average marks: ");
    scanf("%f", &avg);

    if (avg >= 90)
        printf("Grade: A\n");
    else if (avg >= 75)
        printf("Grade: B\n");
    else if (avg >= 60)
        printf("Grade: C\n");
    else if (avg >= 45)
        printf("Grade: D\n");
    else
        printf("Grade: F\n");

    return 0;
}
