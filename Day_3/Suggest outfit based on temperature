#include <stdio.h>

int main() {
    float temp;
    printf("Enter temperature in Celsius: ");
    scanf("%f", &temp);

    if (temp < 10)
        printf("Wear a Jacket\n");
    else if (temp >= 10 && temp <= 20)
        printf("Wear a Sweater\n");
    else
        printf("Wear a T-shirt\n");

    return 0;
}
